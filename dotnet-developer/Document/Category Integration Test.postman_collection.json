				{
					"name": "Create Category",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseJson = pm.response.json();\r",
									"\r",
									"pm.test(\"Status code should be 200 \", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response must have created model data\", function () {\r",
									"    pm.expect(responseJson.Data.name).to.eql(\"โทรศัพท์\");\r",
									"    pm.expect(responseJson.Data.type).to.eql(1);\r",
									"    pm.globals.set(\"id\", responseJson.Data.id);\r",
									"});\r",
