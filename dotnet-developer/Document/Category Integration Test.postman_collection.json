				{
					"name": "Check Data",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const categoryId = pm.globals.get(\"id\");\r",
									"const responseJson = pm.response.json();\r",
									"\r",
									"pm.test(\"Status code should be 200 \", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response must have created data\", function () {\r",
									"    var newCategory = _.find(responseJson.Data.list,  {\r",
									"        id: categoryId\r",
									"    })\r",
									"    pm.expect(newCategory.id).to.eql(categoryId);\r",
									"    pm.expect(newCategory.name).to.eql(\"โทรศัพท์\");\r",
									"    pm.expect(newCategory.type).to.eql(3);\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
